<h2>Search Authors</h2>
<form action="/authors" method="GET">
    <label for="">Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>"></input>
    <button type="submit">Search</button>
</form>
<br></br>
<% authors.forEach(author => { %>
    <div><%= author.name %></div>
<% }) %>